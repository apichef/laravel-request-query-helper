{
    "name": "milroyfraser/laravel-json-api-query-params",
    "type": "library",
    "description": "Easily interact with JSON-API query params.",
    "keywords": [
        "milroyfraser",
        "laravel-json-api-query-params"
    ],
    "homepage": "https://github.com/milroyfraser/laravel-json-api-query-params",
    "license": "MIT",
    "authors": [
        {
            "name": "Milroy E. Fraser",
            "email": "milroy@outlook.com",
            "homepage": "https://github.com/milroyfraser",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelJsonApiQueryParams\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelJsonApiQueryParams\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
