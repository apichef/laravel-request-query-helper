{
    "name": "apichef/laravel-request-query-helper",
    "type": "library",
    "description": "Easily interact with JSON-API query params.",
    "keywords": [
        "milroyfraser",
        "laravel-json-api-query-params"
    ],
    "homepage": "https://github.com/apichef/laravel-request-query-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Milroy E. Fraser",
            "email": "milroy@outlook.com",
            "homepage": "https://github.com/milroyfraser",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.4 || ^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ApiChef\\RequestQueryHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiChef\\RequestQueryHelper\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ApiChef\\RequestQueryHelper\\RequestQueryHelperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
